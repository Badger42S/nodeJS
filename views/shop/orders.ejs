<%- include('../includes/head.ejs') %>

</head>

<body>
    <%- include('../includes/navigation.ejs') %>
    <main>
        <% if(orders.length>0){%>
            <ul>
                <% orders.forEach(order=>{%>
                    <li>
                        <p><%=order.id%></p> 
                        <ul>
                            <% order.products.forEach(item=>{%>
                                <li>
                                    <%=item.title%> (<%=item.orderItem.qty%>)
                                </li>
                            <%})%>
                        </ul>
                    </li>
                <%})%>
            </ul>
        <%} else{%>
            <h1>placeholder</h1>
        <%}%>
    </main>
<%- include('../includes/end.ejs') %>